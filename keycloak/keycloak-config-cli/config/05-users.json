{
    "enabled": true,
    "realm": "mycluster",
    "users": [
        {
        "username": "$(env:PI_ADMIN_USERNAME)",
        "firstName": "$(env:PI_ADMIN_FIRSTNAME)",
        "lastName": "$(env:PI_ADMIN_LASTNAME)",
        "email": "admin@$(env:CLUSTER_DOMAIN)",
        "enabled": true,
        "emailVerified": true,
        "credentials": [
            {
            "type": "password",
            "value": "$(env:PI_ADMIN_PASSWORD)"
            }
        ],
        "realmRoles": [
            "default-roles-mycluster"
        ],
        "groups": [
            "admin"
        ]
        }
    ]
}